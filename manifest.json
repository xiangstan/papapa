{
  "manifest_version": 2,
  "name": "啪啪啪 Click",
  "description": "Search and click Matters Likr Clap Button",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
	  "matches": [
	    "<all_urls>"
	  ],
	  "js": ["papapa.js"]
	}
  ],
  "permissions": [
    "activeTab"
  ]
}